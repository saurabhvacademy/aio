{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "aio": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/aio",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/sitemap.xml",
              "src/index_php_dist.txt",
              "src/index_php_live.txt",
              "src/index_php_uat.txt",
              "src/setTags_dist.php",
              "src/setTags_live.php",
              "src/setTags_uat.php",
              "src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "./node_modules/slick-carousel/slick/slick.scss",
              "./node_modules/slick-carousel/slick/slick-theme.scss",
              "./node_modules/ngx-perfect-scrollbar/dist/lib/perfect-scrollbar.component.css",
              "src/styles.css",
              "./node_modules/ngx-toastr/toastr.css",
              "./node_modules/quill/dist/quill.core.css",
              "./node_modules/quill/dist/quill.snow.css",
              "./src/assets/js/slider/slidersmooth.css",
              "./src/assets/js/slider/aditslider.css",
              "./src/assets/css/perfectscroll.css",
              "./node_modules/@ng-select/ng-select/themes/default.theme.css",
              "./node_modules/perfect-scrollbar/css/perfect-scrollbar.css"

            ],
            "scripts": [
              "./node_modules/jquery/dist/jquery.min.js",
              "./src/assets/js/stream/adapter.js",
              "./src/assets/js/slider/slider.js",
              "./src/assets/js/slider/slidersmooth.js",
              "./node_modules/slick-carousel/slick/slick.min.js",
              "./node_modules/bootstrap/dist/js/bootstrap.min.js",
              "./src/assets/js/stream/Screen-Capturing.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "aio:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "aio:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "aio:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "src/styles.css"
            ],
            "scripts": [],
            "assets": [
              "src/favicon.ico",
              "src/sitemap.xml",
              "src/assets",
              "src/assets/static-pages/widget.html",
              {
                "glob": "widget.html",
                "input": "src/assets/static-pages/",
                "output": "/home/"
              }
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "aio-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "aio:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "aio:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "aio",
  "schematics": {
    "@schematics/angular:component": {
      "styleext": "scss"
    }
  },
  "cli": {
    "analytics": "cceff3a6-aebf-47ae-92d1-5a86afcabb00"
  }
}
